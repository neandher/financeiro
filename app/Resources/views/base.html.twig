<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white {% block body_class %}{% endblock %}">

{{ include('partials/navbar.html.twig') }}

<!-- BEGIN CONTAINER -->
<div class="page-container">

    <!-- BEGIN SIDEBAR -->
    {% include 'partials/sidebar.html.twig' with {ul_page_sidebar_menu_class: (block('ul_page_sidebar_menu_class') is defined ? block('ul_page_sidebar_menu_class') : '' )} %}
    <!-- END SIDEBAR -->

    <!-- BEGIN CONTENT -->
    <div class="page-content-wrapper">
        <!-- BEGIN CONTENT BODY -->
        <div class="page-content">
            <h3 class="page-title">{% block page_title %}{% endblock %}
                <small>{% block page_small_title %}{% endblock %}</small>
            </h3>
            {% block body %}{% endblock %}
        </div>
        <!-- END CONTENT BODY -->
    </div>
    <!-- END CONTENT -->

</div>

<!-- BEGIN FOOTER -->
{{ include('partials/footer.html.twig') }}
<!-- END FOOTER -->

